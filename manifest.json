{
 "manifest_version": 2,
 "name": "GetDistracted",
 "description": "Google Chrome Productivity Extension",
 "version": "0.0.0.1",
 "author": "Jackson Yuan",

 "icons": {
   "16": "icons/icon16x16.png",
   "48": "icons/icon48x48.png",
   "128": "icons/icon128x128.png"
 },

 "browser_action": {
    "default_icon": "icons/icon16x16.png",   
    "default_title": "GetDistracted",
    "default_popup": "popup.html"
 },

 "background": {
   "scripts": ["src/js/background.js"],
   "persistent": true
 },

 "permissions": [
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "cookies",
    "tabs", 
    "<all_urls>"
 ]
}