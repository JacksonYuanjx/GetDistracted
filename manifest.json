{
 "manifest_version": 2,
 "name": "GetDistracted",
 "description": "Google Chrome Productivity Extension",
 "version": "1",
 "author": "Jackson Yuan",

"browser_action": {
    "default_icon": "tab-icon.png",   
    "default_title": "GetDistracted",
    "default_popup": "popup.html"
 },

 "background": {
   "scripts": ["background.js"],
   "persistent": true
 },

 "permissions": [
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "cookies",
    "tabs", 
    "<all_urls>"
 ]
}