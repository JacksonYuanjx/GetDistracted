{
 "manifest_version": 2,
 "name": "GetDistracted",
 "description": "LOREM IPSUM",
 "version": "1",
 "author": "Jackson Yuan",
 "content_security_policy": "script-src 'self' https://cdnjs.cloudflare.com/ajax/libs/gsap/latest/TweenMax.min.js; object-src 'self'",
"browser_action": {
    "default_icon": "tab-icon.png",   
    "default_title": "GetDistracted",
    "default_popup": "popup.html"
 },
 "permissions": [
    "activeTab",
    "storage",
    "cookies",
    "tabs", 
    "http://*/*", 
    "https://*/*"
 ]
}